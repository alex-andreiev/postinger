<h1>RSS feeds</h1>

<table>

  <% @rssfeeds.each do |rss| %>

  <tr>
    <td><%= rss.title %></td>
    <td><%= rss.url %></td>
    <td><%= link_to 'Show', rss %></td>
    <td><%= link_to 'Edit', edit_rssfeed_path(rss) %></td>
    <td><%= link_to 'Destroy', rss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>

    <% end %>

</table>

<%= link_to 'Add RSS feed', new_rssfeed_path %><br />
<%= link_to 'Back', root_path %><br />


